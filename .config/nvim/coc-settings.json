  "intelephense.client.disableInlayHints": false,
  "intelephense.completion.insertUseDeclaration": true,
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.missingProps": true,
  "go.goplsOptions": {
    "completeUnimported": true
  }
